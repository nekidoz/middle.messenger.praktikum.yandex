{
  "extends": "airbnb", // Здесь вы указываете, что наследуетесь от набора правил
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": 2,
    "max-params": [2, 3],
    "indent": ["error", 4, {"SwitchCase": 1}],
    "object-curly-newline": "off",
    "no-alert": "off"
  },
  "settings": {
    "react": {
      "version": "18.3.1"
    }
  },
  "env": {
        "browser": true
  }
} 
